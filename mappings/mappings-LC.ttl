<Biomarker_Positive>
  a rr:TriplesMap;
  rml:logicalSource [
    rml:source <RDB_source>;
    rr:sqlVersion rr:SQL2008;
    rml:query """SELECT DISTINCT t1.medicalrecord_marker, t2.marmolre FROM `Diagnosis_markers` t1, Molecular_markers t2 WHERE t1.ID_marker = t2.ID_marker AND t2.bioStatus = 1""";
  ];
  rr:subjectMap [
    rr:template "http://example.org/lungCancer/entity/{medicalrecord_marker}_Patient";
  ];
  rr:predicateObjectMap [
    rr:predicate lungCancer:hasBiomarker;
    rr:objectMap [
      rr:template "http://example.org/lungCancer/entity/{marmolre}_Positive";
    ]
  ].

